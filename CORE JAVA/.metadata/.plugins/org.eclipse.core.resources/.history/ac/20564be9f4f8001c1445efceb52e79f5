package array;

public class ArrayExample {
	public static int
    missingTerms(int[] nums)
    {
        int n=nums.length;
        int sum=((n+1)*(n+2))/2;
        for(int i=0;i<n;i++)
          sum-=nums[i];
        return sum;
    }
	public static void main(String[] args) {
		int arr[] = { 23, 25, 27, 30 };

		
			
	}
}
