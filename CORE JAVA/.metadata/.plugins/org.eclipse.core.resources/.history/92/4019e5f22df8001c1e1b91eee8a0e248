package P1;

public class Employee {
	int id;
	String name;
	float sal;

	public Employee(int id, String name) {
		super();
		this.id = id;
    	this.name = name;
		this.sal = sal;
	}

	void show() {
		System.out.println(id + "" + "" + sal);
	}

}
class PartTimeEmp extends Employee{
	int fsal,enhrs,hrs_sal;
	public PartTimeEmp(int id,int name,int sal,int hrs_sal) {
		super(id,name,sal);
		this.id=id;
		this.name=name;
		this.sal=sal;
	}
	void calSal()
	{
		sal=fsal+(enhrs*hrs_sal);
	}
	
}
public class InheritanceExample1{
	public static void main(String args[]) {
	 PartTimeEmp p=new PartTimeEmp(0, 0, 0, 0)	
	}
}
