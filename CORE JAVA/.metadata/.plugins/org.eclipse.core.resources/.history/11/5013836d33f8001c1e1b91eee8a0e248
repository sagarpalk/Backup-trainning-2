package P1;

class Shape
{
	float ans;
	void area(float r)
	{
		ans=3.14f*r*r;
		System.out.println("Area:"+ans);
	}
	void area(int s)
	{
		ans=s*s;
		System.out.println("Area"+ans);
	}
	void area(float h,float b)
	{
		ans=2*b*h;
		System.out.println("Area"+ans );
	}
}
public class methodOverloading {

}
